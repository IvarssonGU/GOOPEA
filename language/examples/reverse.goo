enum List = Nil, Cons(Int, List);

Int: List
build x = match x == 0 {
    True: Nil,
    False: Cons(x, build(x - 1))
};
    
fip (List, List): List
rev_h(list, acc) =
    match list {
        Nil: acc,
        Cons(x, xs): rev_h(xs, Cons(x, acc))
    };

fip List: List
rev list = rev_h(list, Nil);

List: Int
sum list = match list {
    Nil: 0,
    Cons(value, rest): value + sum(rest)
};

(): Int
main = sum(rev(build(100)));