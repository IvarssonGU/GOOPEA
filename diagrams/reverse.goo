data List = Empty | Cons(Int, List)
    
Reverse(list, acc) =
    match list {
        Empty: acc,
        Cons(val, next): 
            let acc = Cons(val, acc) in 
            Reverse(next, acc)
    }

Main() =
    let temp = Cons(2, Empty) in
    let temp = Cons(1, temp) in
    Reverse(temp, Empty)